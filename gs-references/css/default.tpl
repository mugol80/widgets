{$mainClassName} .{$cssClassPrefix}content * {
	color: #fff;
}

{$mainClassName} .{$cssClassPrefix}mask {
	background: rgba(0,0,0,.5);
}

{$mainClassName} .{$cssClassPrefix}info .{$cssClassPrefix}line {
	background-color: #a79878;
}

{$mainClassName} .{$cssClassPrefix}details .{$cssClassPrefix}website {
	border-top: 1px solid rgba(255,255,255,.4);
}

{$mainClassName} .{$cssClassPrefix}details .{$cssClassPrefix}client {
border-top: 1px solid rgba(255,255,255,.4);
}

{$mainClassName} .{$cssClassPrefix}column a {
	text-decoration: none;
}

{$mainClassName} .{$cssClassPrefix}paginationList a {
	color: #333;
	background: #f5f5f5;
	border-bottom:solid 2px #fff;
}

{$mainClassName} .{$cssClassPrefix}paginationList a:hover {
	color: #000;
	background: #eee;
	border-bottom:solid 2px #a79878;
}

{$mainClassName} .{$cssClassPrefix}paginationList .{$cssClassPrefix}active a {
	color: #000;
	background: #eee;
	border-bottom:solid 2px #a79878;
}

